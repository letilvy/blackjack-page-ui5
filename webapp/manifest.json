{
	"_version": "1.0.0",
	"sap.app": {
		"id": "ase.ui5.blackjack",
		"type": "application",
		"resources": "resources.json",
		"i18n": "i18n/i18n.properties",
		"title": "Blackjack",
		"description": "Blackjack for ASE",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/services/odata/blackjack",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	
	"sap.fiori": {
		"registrationIds": ["F1919"],
		"archeType": "transactional"
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://paper-plane",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ase.ui5.blackjack.view.App",
			"type": "XML",
			"id": "app_blackjack"
		},
		"dependencies": {
			"minUI5Version": "1.38.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.38.0"
				},
				"sap.m": {
					"minVersion": "1.38.0"
				},
				"sap.ui": {
					"minVersion": "1.38.5",
					"lazy": true
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ase.ui5.blackjack.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ase.ui5.blackjack.view",
				"controlId": "app_blackjack",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "lobby",
				"target": ["lobby"]
			}],
			"targets": {
				"lobby": {
					"viewName": "Lobby",
					"viewLevel": 0,
					"controlAggregation": "pages"
				}
			}
		}
	}
}